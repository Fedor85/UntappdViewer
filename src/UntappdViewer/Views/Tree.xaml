<UserControl x:Class="UntappdViewer.Views.Tree"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UntappdViewer.Views"
             xmlns:prism="http://prismlibrary.com/"
             xmlns:properties="clr-namespace:UntappdViewer.Properties"
             xmlns:this="clr-namespace:UntappdViewer.ViewModels"
             mc:Ignorable="d" 
             prism:ViewModelLocator.AutoWireViewModel="True"
             d:DesignHeight="600" d:DesignWidth="250">
    <ItemsControl>
        <Label Content="{x:Static properties:Resources.Checkins}" FontWeight="Bold"/>
        <TreeView ItemsSource ="{Binding TreeItems}"  Background="Transparent" BorderBrush="Transparent"/>
    </ItemsControl>
</UserControl>
